{
  "manifest_version": 2,

  "name": "Radio Welat FM",
  "short_name": "Welat FM",
  "description": "Chrome Extension for radio WelatFM",
  "version": "1.0.3",


  "browser_action": {
    "default_icon": "media/icon.png",
    "default_popup": "popup.html",
    "default_title": "Livestream welatFM"
  },

  "icons": {
    "16": "media/icon.png",
    "48": "media/icon.png",
    "128": "media/icon.png"
  },

  "permissions": [
    "tabs",
    "http://www.google.com/",
    "http://www.welat-press.com/*" ,
    "http://www.welat.fm/" ,
    "webNavigation",
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "update_url": "https://clients2.google.com/service/update2/crx",
  "author": "Ahmad@welat-press.com"
}